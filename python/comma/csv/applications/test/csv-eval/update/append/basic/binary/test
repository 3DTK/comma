#!/bin/bash

scriptname=$0
source $( type -p comma-application-util ) || { echo "$scriptname: failed to load comma-application-util" >&2; exit 1; }

csv-to-bin 2d | csv-eval --fields=x,y --binary=2d 'x=x+1;y=x-0.1;a=x+y' | csv-from-bin 3d | name-value-from-csv x,y,a --strict --line-number --prefix output | sed 's/"//g'
comma_status_ok
echo "status=$?"
echo "eps=1e-8"
