#!/bin/bash

scriptname=$0
source $( type -p comma-application-util ) || { echo "$scriptname: failed to load comma-application-util" >&2; exit 1; }

csv-eval --fields=,t,,x,n --format=,t,,,ui, 't+=1;x=x-0.1;n=n+1'  | name-value-from-csv dummy1,t,dummy2,x,n,dummy3 --strict --line-number --prefix output | sed 's/"//g'
comma_status_ok
echo "status=$?"
echo "eps=1e-8"
