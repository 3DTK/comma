#!/bin/bash

scriptname=$( readlink -f $0 )
source $( type -p comma-application-util ) || { echo "$scriptname: failed to load comma-application-util" >&2; exit 1; }

csv-eval --fields=,b --select='a < 2*b' --default-values='a=5;b=100' | name-value-from-csv dummy,b --line-number --prefix output | sed 's/"//g'
comma_status_ok
echo "status=$?"
