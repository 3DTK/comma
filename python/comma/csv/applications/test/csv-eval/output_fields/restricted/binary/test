#!/bin/bash

scriptname=$( readlink -f $0 )
source $( type -p comma-application-util ) || { echo "$scriptname: failed to load comma-application-util" >&2; exit 1; }

csv-to-bin 2ui | csv-eval --fields=i,j --binary=2ui 'ab = i+j; cd = i-j; ef = i*j' --output-fields='ab,ef' --output-format=2i -v \
    | csv-from-bin 2ui,2i | name-value-from-csv i,j,ab,ef --line-number --prefix output | sed 's/"//g'
comma_status_ok
echo "status=$?"

