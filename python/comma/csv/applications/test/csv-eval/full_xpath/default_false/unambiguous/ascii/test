#!/bin/bash

scriptname=$( readlink -f $0 )
source $( which comma-application-util ) || { echo "$scriptname: failed to source comma-application-util" >&2; exit 1; }

csv-eval --fields=one/x,y --format=2ui 'x+=1; y+=1; a=x+y' --output-format=ui | name-value-from-csv x,y,a --line-number --prefix output | sed 's/"//g'
comma_status_ok
echo "status=$?"
