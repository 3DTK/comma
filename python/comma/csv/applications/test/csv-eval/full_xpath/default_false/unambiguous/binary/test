#!/bin/bash

scriptname=$( readlink -f $0 )
source $( which comma-application-util ) || { echo "$scriptname: failed to source comma-application-util" >&2; exit 1; }

csv-to-bin 4ui | csv-eval --fields=one/x,a/one/y,a/two/z,w --binary=4ui 'x+=1; y+=1; z+=1; w+=1' \
	| csv-from-bin 4ui | name-value-from-csv x,y,z,w --line-number --prefix output | sed 's/"//g'
comma_status_ok
echo "status=$?"
