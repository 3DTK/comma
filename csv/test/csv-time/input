test[0]="echo 2014-03-05T23:00:00.000Z | csv-time --from xsd --to iso"
test[1]="echo 20140305T230000 | csv-time --from iso --to xsd"
test[2]="echo 2014-03-05T23:00:00 | csv-time --from xsd --to seconds"
