lines[0]="echo -e \"20140305T230000\\n20140305T230000\" | csv-time --from iso --to seconds | tr '\\n' '#'"
lines[0]="echo -e \"20140305T230000\\n2014-03-05T23:00:00\" | csv-time --from any --to seconds | tr '\\n' '#'"

