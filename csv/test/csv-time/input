chain[0]="echo 2014-03-05T23:00:00.000Z | csv-time --from xsd --to iso | csv-time --from iso --to xsd | csv-time --from xsd --to seconds"

legacy[0]="echo 20140305T230000,20140305T230000 | csv-time --from iso --to seconds --fields 2"
legacy[1]="echo 20140305T230000,20140305T230000,20140305T230000 | csv-time --from iso --to seconds --fields 2,3"
legacy[2]="echo 20140305T230000,blue,20140305T230000 | csv-time --from iso --to seconds --fields 3"

fields[0]="echo 20140305T230000,20140305T230000 | csv-time --from iso --to seconds --fields a,b"
fields[1]="echo 20140305T230000,20140305T230000 | csv-time --from iso --to seconds --fields ,2"
fields[2]="echo 20140305T230000,20140305T230000 | csv-time --from iso --to seconds --fields 1,"
fields[3]="echo 20140305T230000,blue,20140305T230000 | csv-time --from iso --to seconds --fields ,,3"

iso[0]="echo 20140305T230000 | csv-time --from iso --to seconds"
iso[1]="echo 20140305T230000 | csv-time --from iso-8601-basic --to seconds"

sql[0]="echo '2014-03-05 23:00:00' | csv-time --from sql --to seconds"
sql[1]="echo '2014-03-05 23:00:00' | csv-time --from posix --to seconds"
sql[2]="echo '2014-03-05 23:00:00' | csv-time --from 'ieee-std-1003.1' --to seconds"

xsd[0]="echo '2014-03-05T23:00:00.000Z' | csv-time --from xsd --to seconds"
xsd[1]="echo '2014-03-05T23:00:00.000Z' | csv-time --from 'iso-8601-extended' --to seconds"

