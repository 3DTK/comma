# With --format=
block_indexing/reverse/test[0]/input[0]="a,1,2"
block_indexing/reverse/test[0]/input[0]="a,1,2"
block_indexing/reverse/test[0]/input[0]="a,1,2"
block_indexing/reverse/test[0]/input[0]="b,3,4"
block_indexing/reverse/test[0]/input[0]="c,4,5"
block_indexing/reverse/test[0]/input[0]="c,4,5"
block_indexing/reverse/test[0]/input[0]="d,3,4"
block_indexing/reverse/test[0]/input[0]="d,3,4"
block_indexing/reverse/test[0]/input[0]="d,3,4"
block_indexing/reverse/test[0]/input[0]="d,3,4"
block_indexing/reverse/test[0]/command="csv-block --fields=d,a,block index --format=s[1],ui,ui  --verbose"

# Without --format=
block_indexing/reverse/test[1]/input[0]="a,1,2"
block_indexing/reverse/test[1]/input[0]="a,1,2"
block_indexing/reverse/test[1]/input[0]="a,1,2"
block_indexing/reverse/test[1]/input[0]="b,3,4"
block_indexing/reverse/test[1]/input[0]="c,4,5"
block_indexing/reverse/test[1]/input[0]="c,4,5"
block_indexing/reverse/test[1]/input[0]="d,3,4"
block_indexing/reverse/test[1]/input[0]="d,3,4"
block_indexing/reverse/test[1]/input[0]="d,3,4"
block_indexing/reverse/test[1]/input[0]="d,3,4"
block_indexing/reverse/test[1]/command="csv-block --fields=d,a,block index --verbose"

# No reverse order
