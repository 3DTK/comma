block_indexing/reverse/test[0]/input[0]="d,3,4"
block_indexing/reverse/test[0]/input[0]="d,3,4"
block_indexing/reverse/test[0]/input[0]="d,3,4"
block_indexing/reverse/test[0]/input[0]="d,3,4"
block_indexing/reverse/test[0]/command="csv-block --fields=d,a,block index --format=s[1],ui,ui  --verbose"

