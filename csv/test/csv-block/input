# Testing appending uint32 block number based on one ID field (1st column)
block_append/test[0]/input[0]="a,1,2"
block_append/test[0]/input[0]="a,1,2"
block_append/test[0]/input[0]="a,1,3"
block_append/test[0]/input[0]="a,1,3"
block_append/test[0]/input[0]="a,1,3"
block_append/test[0]/input[0]="b,3,4"
block_append/test[0]/input[0]="c,4,5"
block_append/test[0]/input[0]="c,4,5"
block_append/test[0]/input[0]="d,3,4"
block_append/test[0]/input[0]="d,3,4"
block_append/test[0]/input[0]="d,3,1"
block_append/test[0]/input[0]="d,3,1"
block_append/test[0]/command="csv-block --fields=id, append --starting-block 701"

# Testing appending uint32 block number based on two IDs field (1st and 3rd columns)
block_append/test[1]/input[0]="a,1,2"
block_append/test[1]/input[0]="a,1,2"
block_append/test[1]/input[0]="a,1,3"
block_append/test[1]/input[0]="a,1,3"
block_append/test[1]/input[0]="a,1,3"
block_append/test[1]/input[0]="b,3,4"
block_append/test[1]/input[0]="c,4,5"
block_append/test[1]/input[0]="c,4,5"
block_append/test[1]/input[0]="d,3,4"
block_append/test[1]/input[0]="d,3,4"
block_append/test[1]/input[0]="d,3,1"
block_append/test[1]/input[0]="d,3,1"
block_append/test[1]/command="csv-block --fields=id,,id append --number 801"

# Testing appending uint32 block number based on two IDs field, numbering starting from 1 (default)
block_append/test[2]/input[0]="a,1,2"
block_append/test[2]/input[0]="a,1,2"
block_append/test[2]/input[0]="a,1,3"
block_append/test[2]/input[0]="a,1,3"
block_append/test[2]/input[0]="a,1,3"
block_append/test[2]/input[0]="b,3,4"
block_append/test[2]/input[0]="c,4,5"
block_append/test[2]/input[0]="c,4,5"
block_append/test[2]/input[0]="d,3,4"
block_append/test[2]/input[0]="d,3,4"
block_append/test[2]/input[0]="d,3,1"
block_append/test[2]/input[0]="d,3,1"
block_append/test[2]/command="csv-block --fields=id,,id append "

# The specified block field is incremented
block_increment/test[0]/input[0]="a,1,2"
block_increment/test[0]/input[0]="a,1,2"
block_increment/test[0]/input[0]="a,1,3"
block_increment/test[0]/input[0]="a,1,3"
block_increment/test[0]/input[0]="a,1,3"
block_increment/test[0]/input[0]="b,3,4"
block_increment/test[0]/input[0]="c,4,5"
block_increment/test[0]/input[0]="c,4,5"
block_increment/test[0]/input[0]="d,3,4"
block_increment/test[0]/input[0]="d,3,4"
block_increment/test[0]/input[0]="d,3,1"
block_increment/test[0]/input[0]="d,3,1"
block_increment/test[0]/command="csv-block increment --fields=a,b,block"
# The specified block field is incremented
block_increment/test[1]/input[0]="a,1,2"
block_increment/test[1]/input[0]="a,1,2"
block_increment/test[1]/input[0]="a,1,3"
block_increment/test[1]/input[0]="a,1,3"
block_increment/test[1]/input[0]="a,1,3"
block_increment/test[1]/input[0]="b,3,4"
block_increment/test[1]/input[0]="c,4,5"
block_increment/test[1]/input[0]="c,4,5"
block_increment/test[1]/input[0]="d,3,4"
block_increment/test[1]/input[0]="d,3,4"
block_increment/test[1]/input[0]="d,3,1"
block_increment/test[1]/input[0]="d,3,1"
block_increment/test[1]/command="csv-block increment --fields=a,block,b --format=s[1],ui,ui"
