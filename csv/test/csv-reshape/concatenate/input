basics[0]="seq 1 10 | csv-reshape concatenate -n 3 --discard | tr \\\\n ';' | sed 's#;$##'; (( ${PIPESTATUS[1]} == 0 ))"
basics[1]="seq 1 10 | csv-reshape concatenate -n 4 --discard | tr \\\\n ';' | sed 's#;$##'; (( ${PIPESTATUS[1]} == 0 ))"
# -n value must be greater than one
basics[2]="seq 1 10 | csv-reshape concatenate -n 1 --discard | tr \\\\n ';' | sed 's#;$##'; (( ${PIPESTATUS[1]} == 0 ))"
basics[3]="seq 1 10 | csv-reshape concatenate -n 0 --discard | tr \\\\n ';' | sed 's#;$##'; (( ${PIPESTATUS[1]} == 0 ))"

sliding_window[0]="seq 1 10 | csv-reshape concatenate -n 2 -w --discard | tr \\\\n ';' | sed 's#;$##'; (( ${PIPESTATUS[1]} == 0 ))"
sliding_window[1]="seq 1 10 | csv-reshape concatenate -n 3 -w --discard | tr \\\\n ';' | sed 's#;$##'; (( ${PIPESTATUS[1]} == 0 ))"
sliding_window[2]="seq 1 10 | csv-reshape concatenate -n 9 -w --discard | tr \\\\n ';' | sed 's#;$##'; (( ${PIPESTATUS[1]} == 0 ))"
sliding_window[3]="seq 1 10 | csv-reshape concatenate -n 10 -w --discard | tr \\\\n ';' | sed 's#;$##'; (( ${PIPESTATUS[1]} == 0 ))"
sliding_window[4]="seq 1 10 | csv-reshape concatenate -n 11 -w --discard | tr \\\\n ';' | sed 's#;$##'; (( ${PIPESTATUS[1]} == 0 ))"
sliding_window[0]="seq 1 10 | csv-reshape concatenate -n 1 -w --discard | tr \\\\n ';' | sed 's#;$##'; (( ${PIPESTATUS[1]} == 0 ))"
