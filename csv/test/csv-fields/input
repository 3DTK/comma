numbers[0]="echo a | csv-fields"
numbers[1]="echo a,b | csv-fields"
numbers[2]="echo ,,,a,b | csv-fields"
numbers[3]="echo ,,, | csv-fields"

clear/keep[0]="echo a,b,c,d | csv-fields clear --keep ,,,"
clear/keep[1]="echo a,b,c,d | csv-fields clear --keep a"
clear/keep[2]="echo a,b,c,d | csv-fields clear --keep b"
clear/keep[3]="echo a,b,c,d | csv-fields clear --keep d"
clear/keep[4]="echo a,b,c,d | csv-fields clear --keep a,d"

clear/remove[0]="echo a,b,c,d | csv-fields clear --remove ,,,"
clear/remove[1]="echo a,b,c,d | csv-fields clear --remove a"
clear/remove[2]="echo a,b,c,d | csv-fields clear --remove b"
clear/remove[3]="echo a,b,c,d | csv-fields clear --remove d"
clear/remove[4]="echo a,b,c,d | csv-fields clear --remove a,d"

clear/mask[0]="echo a,b,c,d | csv-fields clear --mask ,,,"
clear/mask[1]="echo a,b,c,d | csv-fields clear --mask a"
clear/mask[2]="echo a,b,c,d | csv-fields clear --mask ,b"
clear/mask[3]="echo a,b,c,d | csv-fields clear --mask ,,,d"
clear/mask[4]="echo a,b,c,d | csv-fields clear --mask a,,,d"

prefix/path[0]="echo a,b,c | csv-fields prefix --path x"
prefix/path[1]="echo ,b,c | csv-fields prefix --path x"
prefix/path[2]="echo a,,c | csv-fields prefix --path x"
prefix/path[3]="echo a,b, | csv-fields prefix --path x"
prefix/path[4]="echo a,b,c | csv-fields prefix --path x --except b"
prefix/path[5]="echo a,b,c | csv-fields prefix --path x --except a,c"
prefix/path[6]="echo a,b,c | csv-fields prefix --path x --except x"
prefix/path[7]="echo a,b,c | csv-fields prefix --path x --fields b"
prefix/path[8]="echo a,b,c | csv-fields prefix --path x --fields a,c"
prefix/path[9]="echo a,b,c | csv-fields prefix --path x --fields x"

prefix/basename[0]="echo a,b,c | csv-fields prefix --basename"
prefix/basename[1]="echo a/b,c/d,e/f | csv-fields prefix --basename"
prefix/basename[2]="echo a/b,c/d,e/f | csv-fields prefix --basename --fields=a/b"
prefix/basename[3]="echo a/b,c/d,e/f | csv-fields prefix --basename --fields=c/d"
prefix/basename[4]="echo a/b,c/d,e/f | csv-fields prefix --basename --fields=e/f"
prefix/basename[5]="echo a/b,c/d,e/f | csv-fields prefix --basename --except=a/b"
prefix/basename[6]="echo a/b,c/d,e/f | csv-fields prefix --basename --except=c/d"
prefix/basename[7]="echo a/b,c/d,e/f | csv-fields prefix --basename --except=e/f"
prefix/basename[8]="echo a/foo/b,c/foo/d,e/f | csv-fields prefix --basename --except=e/f"

strip/path[0]="echo a/foo/b,a/foo/d,e/f | csv-fields strip --path=a"
strip/path[1]="echo a/foo/b,a/foo/d,e/f | csv-fields strip --path=a --except=a/foo/b"
strip/path[2]="echo a/foo/b,a/foo/d,e/f | csv-fields strip --path=a --fields=a/foo/b"

delimiter[0]="echo '###a#b' | csv-fields numbers --delimiter='#'"
delimiter[1]="echo 'a#b#c#d' | csv-fields clear --mask ,,, --delimiter='#'"
delimiter[2]="echo 'a#b#c#d' | csv-fields clear --remove ,,, --delimiter='#'"
delimiter[3]="echo 'a#b#c#d' | csv-fields clear --keep a --delimiter='#'"
delimiter[4]="echo 'a#b#c' | csv-fields prefix --path x --except b --delimiter='#'"
delimiter[5]="echo '#a#b' | csv-fields default --values 1,2,3 --delimiter='#'"

defaults[0]="echo a,b,c | csv-fields default --values 1,2,3"
defaults[1]="echo ,b,c | csv-fields default --values 1,2,3"
defaults[2]="echo ,,c | csv-fields default --values 1,2,3"
defaults[3]="echo ,, | csv-fields default --values 1,2,3"
defaults[4]="echo ,b, | csv-fields default --values 1,2,3"
defaults[5]="echo ,,c | csv-fields default --values 1,2,3"
defaults[6]="echo ,, | csv-fields default --values 1,2"
defaults[7]="echo ,, | csv-fields default --values ,2"
defaults[8]="echo ,, | csv-fields default --values 1,"

cut[0]="echo a,b,c,d,e | csv-fields cut --fields a"
cut[1]="echo a,b,c,d,e | csv-fields cut --fields b"
cut[2]="echo a,b,c,d,e | csv-fields cut --fields e"
cut[3]="echo a,b,c,d,e | csv-fields cut --fields b,d"
cut[4]="echo a,b,c,d,e | csv-fields cut --fields b,,,d"
cut[5]="echo ,b,c,d,e | csv-fields cut --fields b,d"
cut[6]="echo ,b,,d,e | csv-fields cut --fields b,d"
cut[7]="echo ,b,c,d,, | csv-fields cut --fields b,d"
