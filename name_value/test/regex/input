test[0]="( echo a[0]/b/c[0]/d=1 ; echo a[1]/b/c[0]/d=2 ) | name-value-get 'a\\[[1]\\]/b' --regex"
test[1]="echo a[0]/b/c[0]/d=1 | name-value-get 'a\\[[0]\\]' --regex"
test[2]="( echo a/b/c[0]/d=1 ; echo a[1]/b/c[0]/d=2 ) | name-value-get a --regex"
test[3]="(echo x/y[0]=a;echo x/y[1]=b) | name-value-get x/y[0]"
test[4]="(echo x/y[0]=a;echo x/y[1]=b) | name-value-get x/y[1]"
test[5]="(echo x/y[0]=a; echo a/y[0]=b) | name-value-get './y\\[0\\]' | tr '\\n' ';'"
test[6]="(echo x/y[0]=a;echo x/y[1]=b) | name-value-get x | tr '\\n' ';'"
test[7]="(echo x/b/y[0]=4; echo a/b/y[0]=5) | name-value-get './b' | tr '\\n' ';'"
test[8]="(echo x/b/y[0]=4; echo a/b/y[0]=5) | name-value-get x/b/y[0]"
test[9]="(echo x/b/y[0]=4; echo a/b/y[0]=5) | name-value-get a/b/y[0]"
test[10]="(echo y1/x=6; echo y2/x=7) | name-value-get y[12]/x --regex | tr '\\n' ';'"